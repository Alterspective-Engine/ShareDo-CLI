          "command": "sharedo.findOptionSetOptionById",
          "when": "view == SharedoServers && viewItem == optionSets"
        },
        {
          "command": "sharedo.startManualExecution",
          "when": "view == SharedoServers && viewItem == executionOverview",
          "group": "execution@1"
        },
        {
          "command": "sharedo.refreshExecutionMonitoring",
          "when": "view == SharedoServers && viewItem == executionOverview",
          "group": "execution@2"
        },
        {
          "command": "sharedo.showAdvisorIssues",
          "when": "view == SharedoServers && (viewItem == executionOverview || viewItem == advisorIssues)",
          "group": "execution@3"
        },
        {
          "command": "sharedo.exportExecutionStatistics",
          "when": "view == SharedoServers && (viewItem == executionOverview || viewItem == executingPlans)",
          "group": "execution@4"
        },
        {
          "command": "sharedo.downloadAllExecutingWorkflows",
          "when": "view == SharedoServers && (viewItem == executionOverview || viewItem == executingPlans)",
          "group": "execution@5"
        },
        {
          "command": "sharedo.showExecutionDetails",
          "when": "view == SharedoServers && viewItem == executingPlan",
          "group": "details@1"
        },
        {
          "command": "sharedo.downloadWorkflowCode",
          "when": "view == SharedoServers && viewItem == executingPlan",
          "group": "details@2"
        },
        {
          "command": "sharedo.showWorkflowCode",
          "when": "view == SharedoServers && viewItem == executingPlan",
          "group": "details@3"
        },
        {
          "command": "sharedo.cancelExecution",
          "when": "view == SharedoServers && viewItem == executingPlan",
          "group": "details@4"
        }
      ]
    },
    "configuration": {
      "title": "ShareDo",
      "properties": {
        "sharedo.useNewTreeProvider": {
          "type": "boolean",
          "default": false,
          "description": "Use the new refactored tree provider architecture for better performance and maintainability"
        },
        "sharedo.credentials.username": {
          "type": "string",
          "default": "",
          "description": "Saved username for ShareDo authentication (saved after successful login)"
        }
      }
    }
  },
